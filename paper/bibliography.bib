}

@article{miralda-escude_orbital_2002,
	title = {Orbital {Perturbations} of {Transiting} {Planets}: {A} {Possible} {Method} to {Measure} {Stellar} {Quadrupoles} and to {Detect} {Earth}-{Mass} {Planets}},
	volume = {564},
	shorttitle = {Orbital {Perturbations} of {Transiting} {Planets}},
	url = {https://ui.adsabs.harvard.edu//#abs/2002ApJ...564.1019M/abstract},
	doi = {10.1086/324279},
	abstract = {The recent discovery of a planetary transit in the star HD 209458, and the subsequent highly precise observation of the transit light curve with Hubble Space Telescope, is encouraging to search for any phenomena that might induce small changes in the light curve. Here we consider the effect of the quadrupole moment of the parent star and of a possible second planet perturbing the orbit of the transiting planet. Both of these cause a precession of the orbital plane and of the periastron of the planet, which result in a long-term variation of the duration and the period of the transits. For a transiting planet at 0.05 AU, either a quadrupole moment similar to that of the Sun or the gravitational tug from an Earth-like planet on an orbit of semimajor axis {\textasciitilde}0.2 AU and a relative inclination near the optimal 45° would cause a transit duration time derivative of {\textasciitilde}1 s yr{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}.},
	language = {en},
	number = {2},
	urldate = {2018-12-04},
	journal = {The Astrophysical Journal},
	author = {Miralda-Escudé, Jordi},
	month = jan,
	year = {2002},
	pages = {1019},
	file = {Full Text PDF:/Users/luke/Zotero/storage/WNBPIX92/Miralda-Escudé - 2002 - Orbital Perturbations of Transiting Planets A Pos.pdf:application/pdf;Snapshot:/Users/luke/Zotero/storage/CGJJRZQN/ui.adsabs.harvard.edu.html:text/html}
}

@article{pal_periastron_2008,
	title = {Periastron precession measurements in transiting extrasolar planetary systems at the level of general relativity},
	volume = {389},
	url = {https://ui.adsabs.harvard.edu//#abs/2008MNRAS.389..191P/abstract},
	doi = {10.1111/j.1365-2966.2008.13512.x},
	abstract = {Transiting exoplanetary systems are surpassingly important among the planetary systems since they provide the widest spectrum of information for both the planet and the host star. If a transiting planet is on an eccentric orbit, the duration of transits T{\textless}SUB{\textgreater}D{\textless}/SUB{\textgreater} is sensitive to the orientation of the orbital ellipse relative to the line of sight. The precession of the orbit results in a systematic variation in both the duration of individual transit events and the observed period between successive transits, P{\textless}SUB{\textgreater}obs{\textless}/SUB{\textgreater}. The periastron of the ellipse slowly precesses due to general relativity and possibly the presence of other planets in the system. This secular precession can be detected through the long-term change in P{\textless}SUB{\textgreater}obs{\textless}/SUB{\textgreater} (transit timing variations, TTV) or in T{\textless}SUB{\textgreater}D{\textless}/SUB{\textgreater} (transit duration variations, TDV). We estimate the corresponding precession measurement precision for repeated future observations of the known eccentric transiting exoplanetary systems (XO-3b, HD 147506b, GJ 436b and HD 17156b) using existing or planned space-borne instruments. The TDV measurement improves the precession detection sensitivity by orders of magnitude over the TTV measurement. We find that TDV measurements over a approximately 4yr period can typically detect the precession rate to a precision well exceeding the level predicted by general relativity.},
	language = {en},
	number = {1},
	urldate = {2018-11-24},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {P\'al, Andr\'as and Kocsis, Bence},
	month = sep,
	year = {2008},
	pages = {191},
	file = {Full Text PDF:/Users/luke/Zotero/storage/7A5TEW6T/Pál and Kocsis - 2008 - Periastron precession measurements in transiting e.pdf:application/pdf;Snapshot:/Users/luke/Zotero/storage/FZRYMUP4/ui.adsabs.harvard.edu.html:text/html}
}

