	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Southworth, John and Hinse, T. C. and Jørgensen, U. G. and Dominik, M. and Ricci, D. and Burgdorf, M. J. and Hornstrup, A. and Wheatley, P. J. and Anguita, T. and Bozza, V. and Novati, S. Calchi and Harpsøe, K. and Kjærgaard, P. and Liebig, C. and Mancini, L. and Masi, G. and Mathiasen, M. and Rahvar, S. and Scarpetta, G. and Snodgrass, C. and Surdej, J. and Thöne, C. C. and Zub, M.},
	month = jun,
	year = {2009},
	keywords = {binaries: eclipsing, methods: data analysis, methods: observational, planetary systems, stars: individual: WASP-5, techniques: photometric},
	pages = {1023--1031},
