	author = {Belczynski, Krzysztof and Kalogera, Vassiliki and Rasio, Frederic A. and Taam, Ronald E. and Zezas, Andreas and Bulik, Tomasz and Maccarone, Thomas J. and Ivanova, Natalia},
	month = jan,
	year = {2008},
	keywords = {binaries: close, black hole physics, stars: evolution, stars: neutron, white dwarfs, X-rays: binaries},
	pages = {223--260},
	file = {Full Text PDF:/Users/luke/Zotero/storage/X5BI9752/Belczynski et al. - 2008 - Compact Object Modeling with the StarTrack Populat.pdf:application/pdf}
}

@article{zahn_tidal_1977,
	title = {Tidal friction in close binary stars.},
	volume = {500},
	url = {https://ui.adsabs.harvard.edu//#abs/1977A&A....57..383Z/abstract},
	abstract = {We examine various physical mechanisms which may produce tidal friction in close binary stars. We find that the most efficient in stars with convective envelopes is turbulent viscosity retarding the equilibrium tide, and in stars with radiative envelopes the action of radiative damping on the dynamical tide. Theoretical predictions based on these dissipative processes are in good agreement with the rotational velocities and orbital eccentricities observed in close binaries. The results are applied to the X-ray binaries Her X-1 and Cen X-3.},
	language = {en},
	urldate = {2018-11-24},
