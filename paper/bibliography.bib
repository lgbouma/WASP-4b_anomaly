	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Hoyer, S. and L\'opez-Morales, M. and Rojo, P. and Nascimbeni, V. and Hidalgo, S. and Astudillo-Defru, N. and Concha, F. and Contreras, Y. and Servajean, E. and Hinse, T. C.},
	month = sep,
	year = {2013},
	keywords = {planetary systems, techniques: photometric, methods: data analysis, starspots, planets and satellites: individual: WASP-4b, stars: individual: WASP-4},
	pages = {46--58},
	file = {Full Text PDF:/Users/luke/Zotero/storage/SJT44LY7/Hoyer et al. - 2013 - TraMoS project - III. Improved physical parameters.pdf:application/pdf}
}

